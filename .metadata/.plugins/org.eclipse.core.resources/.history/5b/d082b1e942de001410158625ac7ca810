import java.util.List;


public class JobTrackerAssigner implements Runnable {
	private Thread t;
	ZkConnector zkc;
	List<String> list;
	String task;
	public JobTrackerAssigner(String task, List<String> listOfAssign, ZkConnector zkc)
	{
		this.task = task;
		this.list = listOfAssign;
		this.zkc = zkc
	}

	@Override
	public void run() {
		// TODO Auto-generated method stub
		
	}
	
	public void start ()
	{
		if (t == null)
		{
			t = new Thread (this);
			t.start ();
		}
	}
}
